{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "aiverify-apigw",
      "type": "debugpy",
      "request": "launch",
      "cwd": "${workspaceFolder}/aiverify-apigw",
      "module": "aiverify_apigw"
    },
    {
      "name": "ai-verify-portal",
      "type": "node-terminal",
      "request": "launch",
      "command": "npm run dev",
      "cwd": "${workspaceFolder}/ai-verify-portal"
    },
    {
      "name": "test-engine-app",
      "type": "debugpy",
      "request": "launch",
      "cwd": "${workspaceFolder}/test-engine-app",
      "module": "test_engine_app"
    },
    {
      "name": "pytorch",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/stock-plugins/aiverify.stock.image-corruption-toolbox/algorithms/blur_corruptions/aiverify_blur_corruptions/plugin_init.py",
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}/stock-plugins/user_defined_files",
      "args": [
        "--data_path",
        "data/raw_fashion_image_10",
        "--model_path",
        "model/sample_fashion_mnist_pytorch.pt",
        "--model_type",
        "CLASSIFICATION",
        "--ground_truth_path",
        "data/pickle_pandas_fashion_mnist_annotated_labels_10.sav",
        "--ground_truth_label",
        "label",
        "--file_name_label",
        "file_name",
        "--set_seed",
        "10",
        "--corruptions",
        "horizontal_motion_blur",
        "zoom_blur"
      ],
      "justMyCode": false
    },
    {
      "name": "sklearn",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/stock-plugins/aiverify.stock.image-corruption-toolbox/algorithms/blur_corruptions/aiverify_blur_corruptions/plugin_init.py",
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}/stock-plugins/user_defined_files",
      "args": [
        "--data_path",
        "data/raw_fashion_image_10",
        "--model_path",
        "pipeline/multiclass_classification_image_mnist_fashion",
        "--model_type",
        "CLASSIFICATION",
        "--ground_truth_path",
        "data/pickle_pandas_fashion_mnist_annotated_labels_10.sav",
        "--ground_truth_label",
        "label",
        "--file_name_label",
        "file_name",
        "--set_seed",
        "10",
        "--corruptions",
        "horizontal_motion_blur",
        "zoom_blur"
      ],
      "justMyCode": false
    }
  ]
}
