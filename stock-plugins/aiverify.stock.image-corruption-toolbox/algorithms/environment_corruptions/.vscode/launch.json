  "version": "0.2.0",
  "configurations": [
    {
      "name": "environment-corruption",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/aiverify_environment_corruptions/plugin_init.py",
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}/../../../user_defined_files",
      "args": [
        "--data_path",
        "data/raw_fashion_image_10",
        "--model_path",
        "pipeline/multiclass_classification_image_mnist_fashion",
        "--ground_truth_path",
        "data/pickle_pandas_fashion_mnist_annotated_labels_10.sav",
        "--ground_truth",
        "label",
        "--model_type",
        "CLASSIFICATION",
        "--run_pipeline",
        "--annotated_ground_truth_path",
        "data/pickle_pandas_fashion_mnist_annotated_labels_10.sav",
        "--set_seed",
        "10",
        "--file_name_label",
        "file_name",
        "--corruptions",
        "fog",
        "rain",
        "--rain_type",
        "drizzle",
        "heavy",
        "--snow_intensity",
        "1.0",
        "1.5",
        "2.0",
        "2.5",
        "3.0"
      ],
      "justMyCode": false
    }
  ]
}
