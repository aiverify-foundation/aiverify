  "version": "0.2.0",
  "configurations": [
    {
      "name": "blur-corruption",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/aiverify_blur_corruptions/plugin_init.py",
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}/../../../user_defined_files",
      "args": [
        "--data_path",
        "data/raw_fashion_image_10",
        "--model_path",
        "pipeline/multiclass_classification_image_mnist_fashion",
        "--model_type",
        "CLASSIFICATION",
        "--ground_truth_path",
        "data/pickle_pandas_fashion_mnist_annotated_labels_10.sav",
        "--ground_truth",
        "label",
        "--file_name_label",
        "file_name",
        "--set_seed",
        "10"
      ],
      "justMyCode": false
    }
  ]
}
